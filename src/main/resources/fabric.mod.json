  "authors": [
    "DrTheo_ (@dr.theo - discord)"
  ],
  "contact": {
    "repo": "https://github.com/DrTheodor/mc-queue"
  },
  "license": "LGPL-3.0",
  "icon": "assets/queue/icon.png",
  "environment": "*",
  "mixins": [
    "queue.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/CvSkNeQ9uj"
      },
      "badges": [ "library" ]
